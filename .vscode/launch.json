  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "reAttach": true,
      "name": "Launch localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "tmpDir": "/home/tmp"
    },
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/Backend/server.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },


    
  ]
}
